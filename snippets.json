{
  "Create aggregate": {
    "prefix": "ca",
    "body": [
      "CREATE AGGREGATE $1($2)\n\tSFUNC $3\n\tSTYPE $4\n\tFINALFUNC $5\nINITCOND ($6);"
    ]
  },

  "Create function": {
    "prefix": "cf",
    "body": [
      "CREATE OR REPLACE FUNCTION $1 ($2)\n\t\n\\$\\$ /* Embeded lang(JS, Java etc.) */ \\$\\$;"
    ]
  },

  "Create index": {
    "prefix": "ci",
    "body": ["CREATE INDEX ON $1 ($2);"]
  },

  "Create role": {
    "prefix": "cr",
    "body": ["CREATE ROLE IF NOT EXISTS $1\nWITH LOGIN = $2\nAND $3 = $4;"]
  },

  "Create search index": {
    "prefix": "csi",
    "body": ["CREATE SEARCH INDEX IF NOT EXISTS ON $1\nWITH $2 = $3;"]
  },

  "Create table": {
    "prefix": "ct",
    "body": ["CREATE TABLE IF NOT EXISTS $1 (\n\t$2\n);"]
  },

  "Create type": {
    "prefix": "cty",
    "body": ["CREATE TYPE IF NOT EXISTS $1 (\n\t$2\n);"]
  },

  "Create user": {
    "prefix": "cu",
    "body": ["CREATE USER IF NOT EXISTS $1;"]
  },

  "Create materialized view": {
    "prefix": "cmv",
    "body": ["CREATE MATERIALIZED VIEW IF NOT EXISTS $1\nAS SELECT $2"]
  },

  "Create keyspace": {
    "prefix": "ck",
    "body": [
      "CREATE KEYSPACE IF NOT EXISTS $1\nWITH REPLICATION = {\n\t'class' : '$2',\n\t'replication_factor' : 1$3\n};"
    ]
  },

  "Begin Batch": {
    "prefix": "bb",
    "body": ["BEGIN BATCH\n\n$1\n\nAPPLY BATCH;"]
  },

  "list permissions": {
    "prefix": "lp",
    "body": ["LIST ALL PERMISSIONS$1;"]
  },

  "List roles": {
    "prefix": "lr",
    "body": ["LIST ROLES$1;"]
  },

  "List users": {
    "prefix": "lu",
    "body": ["LIST USERS;"]
  }
}
